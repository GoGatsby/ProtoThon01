n=int(input("Enter the Number of rows: "))
l=[]
g=[]
for i in range(1,n+1):
    l=g.copy()
    g.clear()
    for j in range(0,i):
        #print(j)
        if j==0:
            g.append(1)
        elif j==i-1:
            g.append(1)
        else:
            #print(l)
            g.append(l[j-1]+l[j])
    print(g)
    
    
